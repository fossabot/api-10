extend type Query {
    # 查询任务列表
    jobs: [String]
    # 任务状态
    jobState(totpCode: String!, name: String!): GenericPayload
}

extend type Mutation {
    # 暂停任务
    jobPause(totpCode: String!, name: String!): GenericPayload
    # 恢复任务
    jobResume(totpCode: String!, name: String!): GenericPayload
    # 删除任务
    jobRemove(totpCode: String!, name: String!): GenericPayload
    # 触发任务
    jobTrigger(totpCode: String!, name: String!): GenericPayload
}