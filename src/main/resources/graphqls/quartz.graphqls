extend type Query {
    # 查询任务列表
    getJobKeys(group: String,name: String): [JobKey]

    # 删除任务
    removeJob(otpCode: Int!, group: String,name: String): [JobKey]

    # 暂停任务
    pauseJob(otpCode: Int!, group: String,name: String): [JobKey]

    # 恢复任务
    resumeJob(otpCode: Int!, group: String,name: String): [JobKey]
}

type JobKey {
    group: String
    name: String
}