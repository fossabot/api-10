extend type Query {
    # 查询任务列表
    jobKeys(group: String,name: String): [JobKey]
}

extend type Mutation {
    # 暂停任务
    pauseJob(totpCode: Int!, group: String,name: String): [JobKey]
    # 恢复任务
    resumeJob(totpCode: Int!, group: String,name: String): [JobKey]
    # 删除任务
    removeJob(totpCode: Int!, group: String,name: String): [JobKey]
}