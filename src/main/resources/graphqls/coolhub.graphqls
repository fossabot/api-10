extend type Query {
    # 加载所有用户token
    totpTokens(authToken: String!): [TotpToken]
}

extend type Mutation {
    # 微信登录返回auth-totpToken
    wechatLogin(code: String): SessionPayload
    # 添加token
    createToken(authToken: String!,input: CreateTotpTokenInput!): TotpToken
    # 更新token
    updateToken(authToken: String!,input: UpdateTotpTokenInput!): TotpToken
    # 删除token
    removeToken(authToken: String!,id: String!): TotpToken
}